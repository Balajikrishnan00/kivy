#:set s dp(150)


<canvas_Rectangle>
    canvas:
        Rectangle:
            pos:self.center_x-75,self.center_y-75
            size:dp(100),dp(100)
        Rectangle:
            pos:self.center_x-10,self.center_y+56
            size:dp(100),dp(100)
        Rectangle:
            pos:self.x+0,self.y+100*5
            size:dp(100),dp(100)
        Rectangle:
            pos:self.x+100*10,self.y+100*5
            size:dp(100),dp(100)
<canvas_Ellipse>:
    canvas:
        Ellipse:
            pos:self.x+100,self.y+0
            size:dp(100),dp(100)
        Ellipse:
            pos:self.x+25*10,self.center_y+1*10
            size:dp(500),dp(100)

<canvas_Lines>:
